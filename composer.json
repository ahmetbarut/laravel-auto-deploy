{
    "name": "ahmetbarut/laravel-auto-deploy",
    "description": "The tool enables our Laravel applications to automatically deploy when we commit changes, using GitHub webhooks, without the need to connect to the server via SSH.",
    "type": "package",
    "require": {
        "laravel/framework": "11.x || 10.x",
        "laravel/envoy": "^2.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ahmetbarut\\LaravelAutoDeploy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahmetbarut\\LaravelAutoDeploy\\LaravelAutoDeployServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Ahmet Barut",
            "email": "ahmetbarut588@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable"
}
